<video class="lazy" data-src="{{ video.src }}" alt="" width="{{ video.width ?? '1920' }}" height="{{ video.height ?? '1080' }}" controls autoplay muted playsinline poster="{{ video.poster }}">
    <noscript>
        <video src="{{ video.src }}" alt="" width="{{ video.width ?? '1920' }}" height="{{ video.height ?? '1080' }}"></video>
    </noscript>
</video>
