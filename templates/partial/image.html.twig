<img class="lazy" data-src="{{ img.src }}" data-srcset="{{ img.srcset }}" alt="{{ img.image.title }}" width="{{ img.width }}" height="{{ img.height }}">
<noscript>
    <img itemprop="photo" src="{{ img.src }}" alt="{{ img.image.title }}" width="{{ img.width }}" height="{{ img.height }}">
</noscript>
